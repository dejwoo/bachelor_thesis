{
	"inputs" : [
		{
			"name" : "gpsInput",
			"modulePath" : "./gps/gps.module.js", 
			"sourceOption" : { "sampleRate" : -1, "device": "/dev/ttyUSB1", "init": "AT+CGPS=1"}
			"outputs" : ["mqtt","db","iftt"] 
		},{
			"name" : "timeInput",
			"modulePath" : "./time.module.js", 
			"sourceOption" : { "sampleRate" : 1000, "response" : "JSON"}
			"outputs" : ["mqtt"] 
		},{
			"name" : "obdInput",
			"modulePath" : "./obd/obd.module.js", 
			"sourceOption" : { "sampleRate" : 1000, "response" : "JSON"}
			"outputs" : ["db"] 
		}],
	"outputs" : [
		{
			"name" : "mqtt",
			"modulePath" : "./mqtt/mqtt.module.js", 
			"type": "msg" 
		},{
			"name" : "db",
			"modulePath" : "./db/db.module.js", 
			"type": "db" 
			},
		{
			"name" : "iftt",
			"modulePath" : "./iftt/iftt.module.js", 
			"type": "api" 
			}]
}